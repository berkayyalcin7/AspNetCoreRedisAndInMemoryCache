@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit" class="btn btn-primary">Kaydet</button>
</form>


@foreach (var item in Model)
{
    <h2>@item  <a class="btn btn-danger" asp-action="Delete" asp-route-name=@item>Sil</a>  </h2>
}